GET   /*file                 controllers.Assets.versioned(path="/public", file: Asset)
GET   /<[.]+>                     controllers.Assets.at(path:String="/public", file: String = "index.html")
GET   /                     controllers.Assets.at(path:String="/public", file: String = "index.html")
